    var iframeElement = document.createElement("iframe");
    
    // Establecer los atributos del <iframe>
    iframeElement.setAttribute("src", "https://www.googletagmanager.com/ns.html?id="+TagsTrackingId);
    iframeElement.setAttribute("height", "0");
    iframeElement.setAttribute("width", "0");
    iframeElement.setAttribute("style", "display:none;visibility:hidden");
    
    // Agregar el <iframe> al <noscript>
    noscriptElement.appendChild(iframeElement);
    
    // Agregar el <noscript> al final del cuerpo (body) del documento
    document.body.appendChild(noscriptElement);
});




